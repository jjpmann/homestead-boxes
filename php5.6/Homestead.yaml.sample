---
ip: "192.168.10.11"
memory: 2048
cpus: 1
hostname: lmo-homestead-boxes-php5.6
name: lmo-homestead-boxes-php5.6
provider: virtualbox
version: 0.3.3

authorize: ~/.ssh/id_rsa.pub

keys:
    - ~/.ssh/id_rsa

folders:
    - map: "~/lmo-homestead-boxes"
      to: "/home/vagrant/lmo-homestead-boxes"
    - map: "~/Sites/lmo.com"
      to: "/home/vagrant/lmo-com"
sites:
    - map: homestead.app
      to: "/home/vagrant/lmo-homestead-boxes"
    - map: lmo.app
      to: "/home/vagrant/lmo-com/wwwroot"
    - map: digital.app
      to: "/home/vagrant/lmo-com/wwwroot_digital"

databases:
    - homestead

networks:
    - type: public_network
      bridge: "en0: Wi-Fi (AirPort)"

